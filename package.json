{
  "name": "@ariesclark/utils",
  "version": "1.1.2",
  "description": "A couple utility functions",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run -s build:source && npm run -s build:documentation",
    "build:source": "barrelsby -d ./src/functions ./src/math ./src/objects ./src/random && tsc",
    "build:documentation": "typedoc --out docs src/index.ts",
    "lint": "eslint --fix ./src/**/*.ts"
  },
  "keywords": [
    "utility",
    "types",
    "typescript"
  ],
  "author": "ariesclark",
  "license": "MIT",
  "devDependencies": {
    "@ariesclark/eslint-config": "^1.1.0",
    "@types/node": "^17.0.23",
    "eslint": "^8.13.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "barrelsby": "^2.3.4",
    "tslib": "^2.3.1"
  }
}
